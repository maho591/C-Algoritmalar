#include <stdio.h>
/* -----------------------------------------------------------------------------------z den küçük asal sayıları yazcak-------------------------------------------------------------------------------------*/
int main()
{
 int x,y,z;
 
    printf("sayi girin:\n");
    scanf("%d",&z);

 
 for(x=3;x<z;x++)     //n e kadar olan sayılar
 {
 	for(y=2;y<x;y++)   //kendinden önceki sayılar ile bölünüyor mu kontrolü
 	{
 		if(x%y==0){
 		break;
 		}else if(x==y+1){
 			printf("%d\n",x);
 			}
 		
 			
 	}
 }
 return 0;
}






